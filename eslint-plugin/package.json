{
  "name": "eslint-plugin-tsdoc",
  "version": "0.3.0",
  "description": "An ESLint plugin that validates TypeScript doc comments",
  "keywords": [
    "TypeScript",
    "documentation",
    "doc",
    "comments",
    "JSDoc",
    "TSDoc",
    "ESLint",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/tsdoc",
    "directory": "eslint-plugin"
  },
  "homepage": "https://tsdoc.org/",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "heft test --clean",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "dependencies": {
    "@microsoft/tsdoc": "workspace:*",
    "@microsoft/tsdoc-config": "workspace:*"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8",
    "eslint": "^8"
  },
  "devDependencies": {
    "@typescript-eslint/rule-tester": "~8.3.0",
    "@typescript-eslint/utils": "~8.3.0",
    "@rushstack/heft-node-rig": "~2.6.11",
    "@rushstack/heft": "^0.66.13",
    "@types/eslint": "8.40.1",
    "@types/estree": "1.0.1",
    "@types/heft-jest": "1.0.3",
    "@types/node": "14.18.36",
    "eslint": "~8.57.0",
    "eslint-plugin-header": "~3.1.1",
    "typescript": "~5.4.2"
  }
}
